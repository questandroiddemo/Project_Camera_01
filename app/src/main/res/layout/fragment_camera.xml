<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:layout_editor_absoluteX="3dp"
    tools:layout_editor_absoluteY="-8dp">


    <Button
        android:id="@+id/rvc"
        android:layout_width="125dp"
        android:layout_height="51dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="#167FDC"
        android:drawableLeft="@drawable/ic_baseline_camera_rear_24"
        android:text="RVC"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#0B0A0A"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/ffc"
        android:layout_width="125dp"
        android:layout_height="51dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#167FDC"
        android:drawableLeft="@drawable/ic_baseline_camera_front_24"
        android:text="FFC"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rvc"
        app:strokeColor="#0B0A0A"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/cargo"
        android:layout_width="125dp"
        android:layout_height="51dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#167FDC"
        android:drawableLeft="@drawable/ic_baseline_airport_shuttle_24"
        android:text="CARGO"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ffc"
        app:strokeColor="#0B0A0A"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/aux"
        android:layout_width="125dp"
        android:layout_height="51dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#167FDC"
        android:drawableLeft="@drawable/ic_baseline_airport_shuttle_24"
        android:text="AUX"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cargo"
        app:layout_constraintVertical_bias="0.0"
        app:strokeColor="#0B0A0A"
        app:strokeWidth="1dp" />


    <TextView
        android:id="@+id/title"
        android:layout_width="325dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="Tittle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.694"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.094" />

    <TextView
        android:id="@+id/helptext"
        android:layout_width="418dp"
        android:layout_height="50dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="Help Text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextureView
        android:id="@+id/textureView"
        android:layout_width="192dp"
        android:layout_height="364dp"
        android:layout_marginEnd="148dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="191dp"
        android:layout_height="358dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.724"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.301">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
